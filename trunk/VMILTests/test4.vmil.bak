.class public A {
       .fields { str }
       .handler public init() {
      		push-literal "A"
		store-field str
       }
       .handler public getA() {
       		load-field str
		return
       }
}
.class public B extends A {
       .fields { str }
       .handler public init() {
       		load-this
       		push-literal "A.init:0"
		send-message
      		push-literal "B"
		store-field str
       }
       .handler public getB() {
       		load-field str
		return
       }
}
