.class public Program {
       .handler public Main(sys) {
       		.entrypoint
       		.locals { console, o }
		load-argument sys
		push-literal "get-console:0"
		send-message
		store-local console

		load-local console
		push-literal "Bob"
		push-literal "write-line:1"
		send-message

		push-literal "G"
		new-instance
		store-local o

		load-local o
		push-literal "init:0"
		send-message

		load-local console
		load-local o
		push-literal "getA:0"
		send-message
		push-literal "write-line:1"
		send-message
		load-local console
		load-local o
		push-literal "getB:0"
		send-message
		push-literal "write-line:1"
		send-message
		load-local console
		load-local o
		push-literal "getC:0"
		send-message
		push-literal "write-line:1"
		send-message
		load-local console
		load-local o
		push-literal "getD:0"
		send-message
		push-literal "write-line:1"
		send-message
		load-local console
		load-local o
		push-literal "getE:0"
		send-message
		push-literal "write-line:1"
		send-message
		load-local console
		load-local o
		push-literal "getF:0"
		send-message
		push-literal "write-line:1"
		send-message
		load-local console
		load-local o
		push-literal "getG:0"
		send-message
		push-literal "write-line:1"
		send-message
       }
}
